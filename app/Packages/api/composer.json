{
    "name": "platformcore/api",
    "description": "API package for PlatformCore CMS",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "platformcore",
        "platformcore cms",
        "platformcore platform",
        "platformcore api"
    ],
    "homepage": "https://platformcore.com",
    "authors": [
        {
            "name": "PlatformCore Technologies",
            "email": "contact@platformcore.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\Api\\": "src"
        }
    },
    "require": {
        "google/auth": "^1.28"
    },
    "require-dev": {
        "knuckleswtf/scribe": "^5.0",
        "laravel/pint": "^1.5.0",
        "phpunit/phpunit": "^9.0|^10.0|^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "App\\Api\\Providers\\ApiServiceProvider"
            ],
            "aliases": {
                "ApiHelper": "App\\Api\\Facades\\ApiHelper"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
