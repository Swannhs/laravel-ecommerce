{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.2|^8.3",
        "ext-curl": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-zip": "*",
        "bacon/bacon-qr-code": "^2.0|^3.0",
        "barryvdh/laravel-dompdf": "^3.1",
        "doctrine/dbal": "^4.2",
        "google/auth": "^1.28",
        "firebase/php-jwt": "^6.11",
        "google/analytics-data": "^0.23",
        "google/apiclient": "^2.18",
        "guzzlehttp/guzzle": "^7.9",
        "intervention/image": "^3.4",
        "khaled.alshamaa/ar-php": "^7.0",
        "laravel/framework": "12.44.0",
        "laravel/sanctum": "^4.0",
        "laravel/socialite": "^5.0",
        "laravel/tinker": "^2.10",
        "league/flysystem-aws-s3-v3": "^3.0",
        "maatwebsite/excel": "^3.1",
        "maennchen/zipstream-php": "3.1.*",
        "mews/purifier": "^3.3",
        "mobiledetect/mobiledetectlib": "^4.8",
        "mollie/laravel-mollie": "^3.0",
        "mpdf/mpdf": "^8.2",
        "paypal/paypal-checkout-sdk": "^1.0",
        "phpseclib/bcmath_compat": "^2.0",
        "predis/predis": "^3.0",
        "razorpay/razorpay": "^2.9",
        "resend/resend-php": "*",
        "shippo/shippo-php": "^2.0",
        "spatie/simple-excel": "^3.2.0",
        "stripe/stripe-php": "18.2.0",
        "symfony/cache": "^5.4|^6.0|^7.0",
        "symfony/http-client": "^7.2",
        "symfony/mailgun-mailer": "^7.2",
        "symfony/postmark-mailer": "^7.2",
        "symfony/var-exporter": "^5.4|^6.0|^7.0",
        "tightenco/ziggy": "^2.0",
        "twig/twig": "^3.14",
        "yajra/laravel-datatables-buttons": "12.3.1",
        "yajra/laravel-datatables-html": "12.6.0",
        "yajra/laravel-datatables-oracle": "12.6.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "driftingly/rector-laravel": "^2.0",
        "fakerphp/faker": "^1.23",
        "larastan/larastan": "^3.0",
        "laravel/boost": "^1.8",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.24",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "phpunit/phpunit": "^11.5.3",
        "rector/rector": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "App\\Core\\ACL\\": "app/Core/acl/src",
            "App\\Packages\\Api\\": "app/Packages/api/src",
            "App\\Packages\\Assets\\": "app/Packages/assets/src",
            "App\\Core\\Base\\": "app/Core/base/src",
            "App\\Core\\Base\\Tests\\": "app/Core/base/tests",
            "App\\Core\\Chart\\": "app/Core/chart/src",
            "App\\Core\\Dashboard\\": "app/Core/dashboard/src",
            "App\\Packages\\DataSynchronize\\": "app/Packages/data-synchronize/src",
            "App\\Packages\\GetStarted\\": "app/Packages/get-started/src",
            "App\\Core\\Icon\\": "app/Core/icon/src",
            "App\\Packages\\Installer\\": "app/Packages/installer/src",
            "App\\Core\\JsValidation\\": "app/Core/js-validation/src",
            "App\\Core\\Media\\": "app/Core/media/src",
            "App\\Packages\\Menu\\": "app/Packages/menu/src",
            "App\\Packages\\Optimize\\": "app/Packages/optimize/src",
            "App\\Packages\\Page\\": "app/Packages/page/src",
            "App\\Packages\\PluginManagement\\": "app/Packages/plugin-management/src",
            "App\\Packages\\Revision\\": "app/Packages/revision/src",
            "App\\Packages\\SeoHelper\\": "app/Packages/seo-helper/src",
            "App\\Core\\Setting\\": "app/Core/setting/src",
            "App\\Packages\\Shortcode\\": "app/Packages/shortcode/src",
            "App\\Packages\\Sitemap\\": "app/Packages/sitemap/src",
            "App\\Packages\\Slug\\": "app/Packages/slug/src",
            "App\\Core\\Support\\": "app/Core/support/src",
            "App\\Core\\Table\\": "app/Core/table/src",
            "App\\Packages\\Theme\\": "app/Packages/theme/src",
            "App\\Packages\\Widget\\": "app/Packages/widget/src",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Kris\\LaravelFormBuilder\\": "app/Packages/form-builder/src/",
            "Tightenco\\Ziggy\\": "app/Core/base/src/Ziggy"
        },
        "files": [
            "app/Packages/form-builder/helpers/helpers.php"
        ],
        "exclude-from-classmap": [
            "app/Core/",
            "app/Packages/",
            "app/Plugins/",
            "app/Themes/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "Aws\\Script\\Composer\\Composer::removeUnusedServices",
            "Google\\Task\\Composer::cleanup",
            "@php artisan cms:publish:assets"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "setup": [
            "composer install",
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate",
            "@php artisan migrate --force",
            "npm install",
            "npm run prod"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "aws/aws-sdk-php": [
            "S3",
            "Ses",
            "Translate"
        ],
        "google/apiclient-services": [
            "Drive"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": []
}
